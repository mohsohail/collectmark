{
  "manifest_version": 2,
  "name": "Collect Mark",
  "version": "0.1.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["cm_script.js"],
      "css": ["cm_style.css"]
    }
  ],
  "permissions": ["tabs", "bookmarks"],
  "background": {
    "scripts": ["cm_background.js"]
  },
  "browser_action": {
    "default_icon": "collectmark.png"
  }
}
