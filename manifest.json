{
  "manifest_version": 2,
  "name": "CollectMark",
  "version": "0.1.0",
  "web_accessible_resources": ["img/*.jpg"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["script.js"],
      "css": ["style.css"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "collectmark.png",
    "default_popup": "index.html",
    "default_title": "manage bookmarking"
  }
}
